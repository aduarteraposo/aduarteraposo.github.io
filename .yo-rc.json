{
  "generator-nikita": {
    "name": "verdi",
    "private": true,
    "template": "slim",
    "sassCompiler": "libSass",
    "features": [
      "webfonts",
      "svgBackgrounds",
      "gitinfos"
    ],
    "nikitaCssMixins": [
      "respond-to"
    ],
    "nikitaCssExtends": [
      "cf"
    ],
    "formFramework": false,
    "useBuildFolders": true,
    "cleanBuildFolders": true,
    "requirejsPathToBowerComponents": "../../bower_components/"
  }
}